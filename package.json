{
  "name": "frappe-js-py-snippets",
  "displayName": "Frappe JS and Python Snippets",
  "description": "Extension will add shortcuts to use frappe js and py snippets",
  "main": "./dist/extension.js",
  "icon": "profile.png",
  "publisher": "rohitpg18",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.93.0"
  },
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:python"
  ],
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "python",
        "path": "./snippets/python.json"
      }
    ]
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.9.1",
    "typescript": "^5.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rohitpg18/frappe-js-py-snippets.git"
  },
  "scripts": {
    "build": "npx tsc"
  }
}
